<?xml version='1.0' encoding='utf-8'?>
<widget id="kt.free.svae" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>자유부인</name>
    <description>
        a
    </description>
    <author email="k@ug.rec.com" href="http://tt.ka.com">
        a
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <plugin name="com-sarriaroman-photoviewer" spec="^1.1.11" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
